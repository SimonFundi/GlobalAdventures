<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.symonfundi.globaladventures">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GLOBALADVENTURES"
        tools:targetApi="31">
        <activity
            android:name="com.symonfundi.globaladventures.Exit"
            android:exported="false"
            android:label="@string/title_activity_exit"
            android:theme="@style/Theme.GLOBALADVENTURES.NoActionBar" />

        <activity
            android:name="com.symonfundi.globaladventures.ziplinning"
            android:exported="false"
            android:label="@string/title_activity_ziplining"
            android:theme="@style/Theme.GLOBALADVENTURES.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.symonfundi.globaladventures.bowling"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.symonfundi.globaladventures.hiking"
            android:exported="false">
        </activity>
        <activity
            android:name="com.symonfundi.globaladventures.Register"
            android:exported="false" />
        <activity
            android:name="com.symonfundi.globaladventures.Login"
            android:exported="false" />
        <activity
            android:name="com.symonfundi.globaladventures.Karting"
            android:exported="false"
            android:label="@string/title_activity_karting"
            android:theme="@style/Theme.GLOBALADVENTURES.NoActionBar" />


        <activity
            android:name="com.symonfundi.globaladventures.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".payment" />
    </application>

</manifest>